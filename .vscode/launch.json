{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}",
        "--enable-proposed-api",
        "TBillTech.richyaml"
  ],
      "outFiles": [
        "${workspaceFolder}/out/**/*.js"
      ],
      "preLaunchTask": "Build Extension"
    },
    {
      "name": "Run Extension (Windows UI Host)",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
          "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "outFiles": [
          "${workspaceFolder}/out/**/*.js"
      ],
      "preLaunchTask": "Build Extension"
    }
  ]
}
